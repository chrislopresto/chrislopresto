{
  "name": "chrislopresto",
  "version": "0.0.0",
  "description": "Chris LoPresto Monorepo",
  "repository": "https://github.com/chrislopresto/chrislopresto.git",
  "author": "Chris LoPresto",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "format:check": "prettier \"**/*.{ts,tsx,js,jsx,md,mdx,html,css,json}\" --check",
    "format:write": "prettier \"**/*.{ts,tsx,js,jsx,md,mdx,html,css,json}\" --write",
    "prepare": "husky",
    "clean:node_modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2"
  },
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402"
}

    "@remix-run/react": "^2.4.1",
    "@remixicon/react": "^4.0.1",
    "isbot": "^3.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@pandacss/dev": "^0.23.0",
    "@remix-run/dev": "^2.4.1",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
